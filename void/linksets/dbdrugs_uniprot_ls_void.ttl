<> a void:DatasetDescription ;
        dcterms:title "DrugBank Drugs-Uniprot VoID Description"@en ;
        dcterms:description "The VoID Description for a DrugBank Drugs-Uniprot dataset."@en ;
        pav:createdBy <http://orcid.org/0000-0002-1303-2189> ;
        pav:createdOn "2013-10-23T13:30:00Z"^^xsd:dateTime ;
        foaf:primaryTopic :Uniprot-MGI .

<https://github.com/openphacts/ops-platform-setup/raw/master/void/drugbank_void.ttl#drugbank-rdf> void:subset :DB_drugs-Uniprot .

:DB_drugs-Uniprot a void:Linkset ;
        void:linkPredicate skos:exactMatch ;
        dcterms:title "DrugBank Drugs-Uniprot Linkset"@en ;
        dcterms:license <http://creativecommons.org/licenses/by-nd/3.0/> ;
        pav:createdBy <http://orcid.org/0000-0002-1303-2189> ;
        pav:createdOn "2013-10-23T13:30:00Z"^^xsd:dateTime ;
        dul:expresses <http://semanticscience.org/resource/SIO_001171> ;
        pav:version "2.5"; 
        dcterms:description "Linkset relating DrugBank drug entries to Uniprot entries, generated by querying DrugBank (2.5)" ;
        pav:retrievedFrom <http://localhost:8890/sparql/> ;
        pav:retrievedBy <http://orcid.org/0000-0002-1303-2189> ;
        pav:createdWith :db_drugs-uniprot-query ;
        void:subjectsTarget <https://github.com/openphacts/ops-platform-setup/raw/master/void/drugbank_void.ttl#drugbank-rdf> ;
        void:objectsTarget <https://raw.github.com/openphacts/ops-platform-setup/master/void/uniprot/uniprot_2013_05_void.ttl#uniprotkb_rdf> ;
        void:triples "74"^^xsd:integer .

:db_drugs-uniprot-query dcterms:title "DrugBank drugs - UniProt linkset generation SPARQL query" ;
        dcterms:description """PREFIX db: <http://www4.wiwiss.fu-berlin.de/drugbank/resource/drugbank/>
CONSTRUCT {
        ?db_uri skos:exactMatch ?uniprot_uri
} WHERE {
        GRAPH <http://linkedlifedata.com/resource/drugbank> {
                ?db_uri db:swissprotId ?uniprot_uri .
		FILTER (regex(?db_uri,'drugs'))
        }
}""";
